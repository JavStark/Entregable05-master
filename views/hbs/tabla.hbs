
   {{#if hayProds}}
    <table class="table">
  <thead>
    <tr>
      {{!-- <th scope="col">#</th> --}}
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
    {{#each productos}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><img width='50' src={{this.thumbnail}}></td>
    </tr>
  </thead>

        {{/each}}
</table>

        {{else}}
        <p>Sin productos</p>
        {{/if}}
  
   
